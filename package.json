{
  "name": "ghl-marketplace-app-template",
  "version": "1.0.0",
  "description": "A boilerplate for creating GHL marketplace apps",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc && npm run build-ui && mkdir -p ./dist/src/ui && cp -r ./src/ui/dist ./dist/src/ui/",
    "build-ui": "cd src/ui && npm i && npm run build && cd ../../",
    "start": "node dist/src/index.js",
    "dev": "npm run build-ui && ts-node-dev src/index.ts",
    "db:push": "drizzle-kit push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoHighLevel/ghl-marketplace-app-template.git"
  },
  "keywords": [
    "boilerplate",
    "ghl"
  ],
  "author": "GHL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GoHighLevel/ghl-marketplace-app-template/issues"
  },
  "homepage": "https://github.com/GoHighLevel/ghl-marketplace-app-template#readme",
  "dependencies": {
    "@types/pg": "^8.16.0",
    "apify-client": "^2.21.0",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "openai": "^6.14.0",
    "p-limit": "^7.2.0",
    "p-retry": "^7.1.1",
    "pg": "^8.16.3",
    "vue": "^3.3.4",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
